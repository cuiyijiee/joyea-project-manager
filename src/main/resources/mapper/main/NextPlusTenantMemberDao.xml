<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="me.cuiyijie.joyea.dao.main.NextPlusTenantMemberDao">
    <resultMap id="BaseResultMap" type="me.cuiyijie.joyea.model.NextPlusTenantMember">

        <id property="id" column="id"/>
        <id property="name" column="name"/>
        <id property="signature" column="signature"/>
        <id property="email" column="email"/>
        <id property="mobile" column="mobile"/>
        <id property="imageUrl" column="image_url"/>
        <id property="activated" column="activated"/>

    </resultMap>

    <select id="list" resultMap="BaseResultMap">
        SELECT *
        from t_joyea_user
    </select>

    <insert id="insert">
        INSERT INTO t_joyea_user(id, name, signature, email, mobile, image_url, activated)
        VALUES (
                #{item.name},
                #{item.signature},
                #{item.mobile},
                #{item.imageUrl},
                #{item.activated})
    </insert>

</mapper>