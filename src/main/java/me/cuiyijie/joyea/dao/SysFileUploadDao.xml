<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="me.cuiyijie.joyea.dao.SysFileUploadDao">

    <resultMap id="BaseResultMap" type="me.cuiyijie.joyea.model.SysFileUpload">

        <id property="id" column="id"/>
        <id property="originFileName" column="origin_file_name"/>
        <id property="fileSuffix" column="file_suffix"/>
        <id property="createdBy" column="created_by"/>
        <id property="createdAt" column="created_at"/>

    </resultMap>

    <insert id="insert">

        INSERT INTO t_sys_file_upload (id, origin_file_name, file_suffix,created_by, created_at)
        VALUES (
                #{item.id},
                #{item.originFileName},
                #{item.fileSuffix},
                #{item.createdBy},
                #{item.createdAt}
               )
    </insert>

    <select id="selectById" resultMap="BaseResultMap">
        SELECT *
        FROM t_sys_file_upload
        WHERE id = #{id}
    </select>



</mapper>